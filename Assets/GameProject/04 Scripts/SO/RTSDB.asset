  - branch: 1
    dialog: "\uBA3C\uC800 \uC804\uD669\uC744 \uB458\uB7EC\uBCFC\uC218\uC788\uB294
      \uAE30\uBCF8\uC801 \uC774\uB3D9 \uC124\uBA85\uC785\uB2C8\uB2E4."
  - branch: 1
    dialog: "WASD\uD0A4 \uBC0F \uB9C8\uC6B0\uC2A4 \uC6B0\uD074\uB9AD\uC744 \uB204\uB978\uC0C1\uD0DC\uB85C
      \uB4DC\uB798\uADF8\uD558\uBA74 \uD654\uBA74 \uC774\uB3D9\uC774 \uAC00\uB2A5\uD569\uB2C8\uB2E4."
  - branch: 1
    dialog: "\uD720\uBC84\uD2BC\uC744 \uB204\uB978\uC0C1\uD0DC\uC5D0\uC120 \uD654\uBA74
      \uD68C\uC804 \uBC0F \uD720\uC2A4\uD06C\uB864\uB85C \uC90C\uC778 \uC544\uC6C3\uC744
      \uD560\uC218\uC788\uC2B5\uB2C8\uB2E4."
  - branch: 1
    dialog: "\uD55C\uBC88 \uAC04\uB2E8\uD55C \uC870\uC791\uC744 \uD574\uBCF4\uC138\uC694"
  - branch: 2
    dialog: "\uB2E4\uC74C\uC73C\uB860 \uAC04\uB2E8\uD55C \uC9C4\uD589 \uC124\uBA85\uC744
      \uD574\uB4DC\uB9AC\uACA0\uC2B5\uB2C8\uB2E4."
  - branch: 2
    dialog: "\uC800\uD76C\uAC00 \uC804\uD22C\uC5D0\uC11C \uC0AC\uC6A9\uD558\uB294
      \uC2DC\uC124\uBB3C\uB4E4 \uC785\uB2C8\uB2E4."
  - branch: 2
    dialog: "\uAE30\uBCF8\uC801\uC73C\uB85C \uC800\uD76C\uAC00 \uC9C0\uCF1C\uC57C\uD558\uB294
      \uC131\uACFC \uC0DD\uC0B0 \uAC74\uBB3C \uBCD1\uC601\uACFC \uBC29\uC5B4 \uAC74\uBB3C\uB85C
      \uB098\uB258\uC5B4 \uC9D1\uB2C8\uB2E4"
  - branch: 3
    dialog: "\uD574\uB2F9 \uAC74\uBB3C\uB4E4\uC740 \uD558\uB2E8 \uAC74\uCD95\uD0ED\uC5D0\uC11C
      \uAC74\uBB3C\uC744 \uC120\uD0DD\uD55C\uB4A4 \uAC74\uC124\uD558\uACE0 \uC2F6\uC740
      \uACF5\uAC04\uC5D0 \uC88C\uD074\uB9AD\uC744 \uD558\uBA74 \uAC74\uC124\uC774
      \uC774\uB8E8\uC5B4\uC9D1\uB2C8\uB2E4."
  - branch: 3
    dialog: "\uAC74\uC124\uACFC \uAC19\uC740 \uD589\uB3D9\uB4E4\uC5D0 \uB4E4\uC5B4\uAC00\uB294
      \uB3C8\uB4E4\uC740 \uB9E4 \uC8FC\uAE30\uB9C8\uB2E4 \uC0DD\uC0B0\uAC74\uBB3C\uC5D0\uC11C
      \uC218\uC785\uC774 \uB4E4\uC5B4\uC635\uB2C8\uB2E4"
  - branch: 3
    dialog: "\uD55C\uBC88 \uAC74\uC124\uC744 \uD574\uBCF4\uC2DC\uC8E0"
  - branch: 4
    dialog: "\uD68D\uB4DD\uD55C \uACE8\uB4DC\uB97C \uC18C\uBAA8\uD574 \uC720\uB2DB\uC758
      \uC5C5\uADF8\uB808\uC774\uB4DC\uB97C \uC9C4\uD589\uD558\uAC70\uB098 \uCD5C\uB300
      \uC720\uB2DB,\uAC74\uBB3C \uC218\uB97C \uB298\uB9B4 \uC218 \uC788\uC2B5\uB2C8\uB2E4."
  - branch: 4
    dialog: "\uC804\uD22C\uC5D0\uC11C \uC800\uD76C\uC758 \uC131\uC744 \uC9C0\uD0A4\uACE0
      \uC0C1\uB300\uBC29\uC758 \uC131\uC744 \uBD80\uC218\uBA74 \uAC8C\uC784\uC744
      \uD074\uB9AC\uC5B4\uD569\uB2C8\uB2E4."
  - branch: 4
    dialog: "\uAC04\uB2E8\uD55C \uC124\uBA85\uC774 \uC885\uB8CC \uB418\uC5C8\uC73C\uB2C8
